    #include<bits/stdc++.h>
    using namespace std;
     
    //#define debug
     
    int main()
    {
     
    #ifdef debug
        freopen("C:/Users/jahid/Desktop/New Text Document.txt","r",stdin);
    #endif
     
        int t;
        cin>>t;
        while(t--)
        {
            int n,s=0,oc=0;
            cin>>n;
            for(int i=0;i<n;i++)
            {
                int x;
                cin>>x;
                if(x%2==1)oc++;
            }
        if(oc%2==1 || oc<n && oc!=0)
            cout<<"YES"<<endl;
        else
            cout<<"NO"<<endl;
        }
        return 0;
    }
