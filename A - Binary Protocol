    #include<bits/stdc++.h>
    #include<algorithm>
    using namespace std;
     
    //#define debug
     
    int main()
    {
    #ifdef debug
        freopen("C:/Users/jahid/Desktop/New Text Document.txt","r",stdin);
    #endif
     
        int n,c1=0;
        cin>>n;
        string s;
        cin>>s;
        for(int i=0;i<n;i++)
        {
            if(s[i]=='1')c1++;
            else if(s[i]=='0'){
                cout<<c1;
                c1=0;
            }
        }
        cout<<c1;
    }
