    #include<bits/stdc++.h>
    using namespace std;
     
    //#define debug
     
    int main()
    {
    #ifdef debug
        freopen("input.txt","r",stdin);
    #endif
     
        int t,n,m;
        cin>>t;
        while(t--)
        {
            cin>>n>>m;
            if(n%m==0)
            {
                cout<<"YES"<<endl;
            }
            else
                cout<<"NO"<<endl;
        }
     
        return 0;
    }
