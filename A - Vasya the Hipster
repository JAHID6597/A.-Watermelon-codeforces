    #include<bits/stdc++.h>
    using namespace std;
     
    //#define debug
     
    int main()
    {
    #ifdef debug
        freopen("input.txt","r",stdin);
    #endif
        int a,b;
        cin>>a>>b;
        if(a>b)
            cout<<b<<" "<<(a-b)/2<<endl;
        else if(a<=b)
            cout<<a<<" "<<(b-a)/2<<endl;
    }
