    #include<bits/stdc++.h>
    using namespace std;
     
    //#define debug
     
    int main()
    {
    #ifdef debug
        freopen("input.txt","r",stdin);
    #endif
     
        int t;
        cin>>t;
        while(t--)
        {
            int n,c=0,x;
            cin>>n;
            for(int i=0; i<n; i++)
            {
                cin>>x;
                if(x%2 == 0)c++;
            }
            if(c==n || c==0)
            cout<<"YES"<<endl;
            else
                cout<<"NO"<<endl;
        }
     
        return 0;
    }
