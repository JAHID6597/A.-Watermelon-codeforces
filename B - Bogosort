    #include<bits/stdc++.h>
    using namespace std;
     
    //#define debug
     
    int main()
    {
    #ifdef debug
        freopen("input.txt","r",stdin);
    #endif
     
        int t;
        cin>>t;
        while(t--)
        {
            int n;
            cin>>n;
            int a[n];
            for(int i=0;i<n;i++)
            {
                cin>>a[i];
            }
            sort(a,a+n);
            reverse(a,a+n);
            for(int i=0;i<n;i++)
            {
                cout<<a[i]<<" ";
            }
            cout<<endl;
        }
     
        return 0;
    }
