    #include<bits/stdc++.h>
    using namespace std;
     
    //#define debug
     
    int main()
    {
     
    #ifdef debug
        freopen("C:/Users/jahid/Desktop/New Text Document.txt","r",stdin);
    #endif
     
        int n,i,x,s=0;
        double ans;
        cin>>n;
        for(i=1;i<=n;i++)
        {
            cin>>x;
            s+=x;
        }
        ans = (double)s/n;
        cout<<setprecision(12);
        cout<<ans<<endl;
        return 0;
    }
